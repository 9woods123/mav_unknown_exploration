<?xml version="1.0"?>
<package format="2">
    <name>active_3d_planning_mav</name>
    <version>0.0.0</version>
    <description>MAV functionality for Active path planning for 3D tasks</description>

    <author email="schmluk@mavt.ethz.ch">Lukas Schmid</author>

    <maintainer email="schmluk@mavt.ethz.ch">Lukas Schmid</maintainer>

    <license>BSD</license>

    <buildtool_depend>catkin</buildtool_depend>
    <buildtool_depend>catkin_simple</buildtool_depend>

    <depend>active_3d_planning_core</depend>
    <depend>message_generation</depend>
    <depend>message_runtime</depend>
    <depend>trajectory_msgs</depend>
    <depend>mav_trajectory_generation</depend>
    <depend>mav_trajectory_generation_ros</depend>
    <depend>mav_msgs</depend>
    <depend>std_msgs</depend>
    <depend>sensor_msgs</depend>
